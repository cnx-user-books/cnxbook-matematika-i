                        <m:mrow>
                          <m:mi>x</m:mi>
                          <m:mo stretchy="false">→</m:mo>
                          <m:mn>0</m:mn>
                        </m:mrow>
                      </m:mrow>
                    </m:munder>
                    <m:mrow>
                      <m:mfrac>
                        <m:mrow>
                          <m:mtext>sin</m:mtext>
                          <m:mi>x</m:mi>
                        </m:mrow>
                        <m:mi>x</m:mi>
                      </m:mfrac>
                      <m:mo stretchy="false">=</m:mo>
                      <m:mn>1</m:mn>
                    </m:mrow>
                  </m:mrow>
                </m:mrow>
                <m:mrow/>
              </m:mrow>
              <m:annotation encoding="StarMath 5.0"> size 12{ {"lim"}  cSub { size 8{x rightarrow 0} }  {  {"sin"x}  over  {x} } =1} {}</m:annotation>
            </m:semantics>
          </m:math>
        </para>
        <para xmlns:m="http://www.w3.org/1998/Math/MathML" id="import-auto-id1163789078590"><m:math>
            <m:semantics>
              <m:mrow>
                <m:mrow>
                  <m:mrow>
                    <m:munder>
                      <m:mtext>lim</m:mtext>
                      <m:mrow>
