                                <m:mo stretchy="false">¨</m:mo>
                              </m:mover>
                              <m:mrow>
                                <m:mover accent="true">
                                  <m:mi>x</m:mi>
                                  <m:mo stretchy="false">˙</m:mo>
                                </m:mover>
                                <m:mo stretchy="false">−</m:mo>
                                <m:mover accent="true">
                                  <m:mi>y</m:mi>
