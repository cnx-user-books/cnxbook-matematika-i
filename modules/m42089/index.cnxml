          <para id="id1166071691012">и конечно запишуваме дека</para>
          <para id="id1166061132515"><m:math><m:semantics><m:mrow><m:mstyle fontsize="12pt"><m:mrow><m:mrow><m:msup><m:mi>y</m:mi><m:mrow><m:mi>'</m:mi><m:mi>'</m:mi></m:mrow></m:msup><m:mo stretchy="false">=</m:mo><m:mfrac><m:mrow><m:mover accent="true"><m:mi>y</m:mi><m:mo stretchy="false">¨</m:mo></m:mover><m:mrow><m:mover accent="true"><m:mi>x</m:mi><m:mo stretchy="false">˙</m:mo></m:mover><m:mo stretchy="false">−</m:mo><m:mover accent="true"><m:mi>y</m:mi><m:mo stretchy="false">˙</m:mo></m:mover></m:mrow><m:mover accent="true"><m:mi>x</m:mi><m:mo stretchy="false">¨</m:mo></m:mover></m:mrow><m:msup><m:mover accent="true"><m:mi>x</m:mi><m:mo stretchy="false">˙</m:mo></m:mover><m:mstyle fontsize="8pt"><m:mrow><m:mn>3</m:mn></m:mrow></m:mstyle></m:msup></m:mfrac></m:mrow></m:mrow></m:mstyle><m:mrow/></m:mrow><m:annotation encoding="StarMath 5.0"> size 12{ { {y}} sup { '' }= {  { { ddot  {y}} { dot  {x}} -  { dot  {y}} { ddot  {x}}}  over  { { dot  {x}} rSup { size 8{3} } } } } {}</m:annotation></m:semantics></m:math>.</para>
