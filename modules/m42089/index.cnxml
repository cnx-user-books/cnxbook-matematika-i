                                <m:mo stretchy="false">˙</m:mo>
                              </m:mover>
                              <m:mover accent="true">
                                <m:mi>x</m:mi>
                                <m:mo stretchy="false">˙</m:mo>
                              </m:mover>
                            </m:mfrac>
                          </m:mfenced>
                          <m:mo stretchy="false">=</m:mo>
                          <m:mfrac>
                            <m:mi>d</m:mi>
                            <m:mstyle fontstyle="italic">
                              <m:mrow>
                                <m:mtext>dt</m:mtext>
                              </m:mrow>
                            </m:mstyle>
