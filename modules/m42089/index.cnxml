                <m:annotation encoding="StarMath 5.0"> size 12{ { {y}} sup { '' }= {  {d}  over  { ital "dx"} }  left ( {  { ital "dy"}  over  { ital "dx"} }  right )= {  {d}  over  { ital "dx"} }  left ( {  { { dot  {y}}}  over  { { dot  {x}}} }  right )= {  {d}  over  { ital "dt"} }  {  { ital "dt"}  over  { ital "dx"} }  left ( {  { { dot  {y}}}  over  { { dot  {x}}} }  right )= {  {d}  over  { ital "dt"} }  left ( {  { { dot  {y}}}  over  { { dot  {x}}} }  right ) cdot  {  {1}  over  { {  { ital "dx"}  over  { ital "dt"} } } } = {  { { ddot  {y}} { dot  {x}} -  { dot  {y}} { ddot  {x}}}  over  { { dot  {x}} rSup { size 8{2} } } }  cdot  {  {1}  over  { { dot  {x}}} } = {  { { ddot  {y}} { dot  {x}} -  { dot  {y}} { ddot  {x}}}  over  { { dot  {x}} rSup { size 8{3} } } } } {}</m:annotation>
              </m:semantics>
            </m:math>
