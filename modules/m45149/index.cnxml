        <para xmlns:m="http://www.w3.org/1998/Math/MathML" id="import-auto-id1171417265337"><m:math><m:semantics><m:mrow><m:mrow><m:mrow><m:mi>y</m:mi><m:mrow><m:mrow><m:mfenced open="(" close=")"><m:mfrac><m:msqrt><m:mn>3</m:mn></m:msqrt><m:mn>3</m:mn></m:mfrac></m:mfenced><m:mo stretchy="false">=</m:mo><m:mfrac><m:mrow><m:mn>3</m:mn><m:mrow><m:msup><m:mfenced open="(" close=")"><m:mfrac><m:msqrt><m:mn>3</m:mn></m:msqrt><m:mn>3</m:mn></m:mfrac></m:mfenced><m:mrow><m:mn>2</m:mn></m:mrow></m:msup><m:mo stretchy="false">+</m:mo><m:mn>1</m:mn></m:mrow></m:mrow><m:mrow><m:mn>3</m:mn><m:mfrac><m:msqrt><m:mn>3</m:mn></m:msqrt><m:mn>3</m:mn></m:mfrac></m:mrow></m:mfrac></m:mrow><m:mo stretchy="false">=</m:mo><m:mfrac><m:mrow><m:mn>2</m:mn><m:msqrt><m:mn>3</m:mn></m:msqrt></m:mrow><m:mn>3</m:mn></m:mfrac></m:mrow></m:mrow></m:mrow><m:mrow/></m:mrow><m:annotation encoding="StarMath 5.0"> size 12{y left ( {  { sqrt {3} }  over  {3} }  right )= {  {3 left ( {  { sqrt {3} }  over  {3} }  right ) rSup { size 8{2} } +1}  over  {3 {  { sqrt {3} }  over  {3} } } } = {  {2 sqrt {3} }  over  {3} } } {}</m:annotation></m:semantics></m:math> и <m:math><m:semantics><m:mrow><m:mrow><m:mrow><m:mi>y</m:mi><m:mrow><m:mrow><m:mfenced open="(" close=")"><m:mrow><m:mo stretchy="false">−</m:mo><m:mfrac><m:msqrt><m:mn>3</m:mn></m:msqrt><m:mn>3</m:mn></m:mfrac></m:mrow></m:mfenced><m:mo stretchy="false">=</m:mo><m:mfrac><m:mrow><m:mn>3</m:mn><m:mrow><m:msup><m:mfenced open="(" close=")"><m:mrow><m:mo stretchy="false">−</m:mo><m:mfrac><m:msqrt><m:mn>3</m:mn></m:msqrt><m:mn>3</m:mn></m:mfrac></m:mrow></m:mfenced><m:mrow><m:mn>2</m:mn></m:mrow></m:msup><m:mo stretchy="false">+</m:mo><m:mn>1</m:mn></m:mrow></m:mrow><m:mrow><m:mrow><m:mo stretchy="false">−</m:mo><m:mn>3</m:mn></m:mrow><m:mfrac><m:msqrt><m:mn>3</m:mn></m:msqrt><m:mn>3</m:mn></m:mfrac></m:mrow></m:mfrac></m:mrow><m:mo stretchy="false">=</m:mo><m:mrow><m:mo stretchy="false">−</m:mo><m:mfrac><m:mrow><m:mn>2</m:mn><m:msqrt><m:mn>3</m:mn></