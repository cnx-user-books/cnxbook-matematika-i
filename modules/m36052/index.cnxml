                              <m:mo stretchy="false">→</m:mo>
                              <m:mrow>
                                <m:mi>π</m:mi>
                                <m:mo stretchy="false">/</m:mo>
                                <m:mn>2</m:mn>
                              </m:mrow>
                            </m:mrow>
                          </m:mrow>
                        </m:mstyle>
                      </m:munder>
                      <m:mo stretchy="false">(</m:mo>
                      <m:mtext>tan</m:mtext>
                      <m:mi>x</m:mi>
                      <m:msup>
                        <m:mo stretchy="false">)</m:mo>
                        <m:mstyle fontsize="8pt">
                          <m:mrow>
                            <m:mrow>
                              <m:mn>2x</m:mn>
